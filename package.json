{
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:php",
    "onLanguage:html",
    "onLanguage:java",
    "workspaceContains:**/*",
    "onStartupFinished",
    "onUri",
    "onCommand:meiko.setApiKey",
    "onCommand:meiko.supabaseLogin",
    "onCommand:meiko.supabaseLogout",
    "onView:meiko.chatView"
  ],
  "author": "Zaky",
  "badges": [
    {
      "description": "Meiko",
      "href": "https://github.com/faizinuha/Meiko",
      "url": "https://img.shields.io/badge/Meiko-Pro-blue"
    }
  ],
  "bin": {
    "coders-crud": "cli/crud-installer.js",
    "coders-delete": "cli/delete-solid.js",
    "coders-solid": "cli/coders-solid.js"
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    },
    "virtualWorkspaces": true
  },
  "categories": [
    "Programming Languages",
    "AI",
    "Snippets",
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "meiko.setApiKey",
        "title": "Meiko: Set Gemini API Key",
        "category": "Meiko"
      },
      {
        "command": "meiko.supabaseLogin",
        "title": "Meiko: Supabase Login",
        "category": "Meiko"
      },
      {
        "command": "meiko.supabaseLogout",
        "title": "Meiko: Supabase Logout",
        "category": "Meiko"
      },
      {
        "command": "meiko.supabaseRefreshDbView",
        "title": "Refresh Supabase DB Explorer",
        "icon": "$(refresh)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "meiko-sidebar",
          "title": "Meiko",
          "icon": "assets/icons1.jpeg"
        },
        {
          "id": "supabase-db-explorer",
          "title": "Supabase",
          "icon": "media/supabase.png"
        }
      ]
    },
    "views": {
      "meiko-sidebar": [
        {
          "icon": "media/image_2025-10-08_145045074.svg",
          "id": "meiko.chatView",
          "name": "Chat",
          "type": "webview"
        }
      ],
      "supabase-db-explorer": [
        {
          "icon": "media/image_2025-10-08_145246818.svg",
          "initialSize": 400,
          "id": "supabase.tablesView",
          "name": "Database"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "meiko.supabaseRefreshDbView",
          "when": "view == supabase.tablesView",
          "group": "navigation"
        }
      ]
    },
    "documentation": {
      "releaseNotes": "./CHANGELOG.md"
    },
    "snippets": [
      {
        "language": "html",
        "path": "./src/html-snippets.json"
      },
      {
        "language": "php",
        "path": "./src/php-snippets.json"
      },
      {
        "language": "php",
        "path": "./src/laravel-snippets.json"
      },
      {
        "language": "java",
        "path": "./src/java-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./src/javascript-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./src/vue-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./src/ReactNative-snippets.json"
      },
      {
        "language": "python",
        "path": "./src/python-snippets.json"
      }
    ]
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "node-fetch": "^2.7.0",
    "ora": "^8.1.1"
  },
  "description": "ðŸš€ Advanced AI-powered VS Code ",
  "devDependencies": {
    "@tensorflow/tfjs-node": "^4.4.0",
    "@types/node": "^22.8.4",
    "@types/vscode": "^1.95.0",
    "electron-builder": "^25.1.8",
    "openai": "^4.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.0.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "displayName": "Meiko",
  "engines": {
    "node": ">=14.0.0",
    "vscode": ">=1.95.0"
  },
  "icon": "assets/icons1.jpeg",
  "id": "ZakyEvenso12",
  "keywords": [
    "CodersLegacy",
    "php",
    "laravel",
    "AI",
    "javascript",
    "vue",
    "react",
    "snippets",
    "productivity",
    "web-development",
    "code-generator",
    "cli-tools",
    "typescript",
    "html"
  ],
  "license": "MIT",
  "main": "./out/extension.js",
  "name": "meiko",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "pricing": "Free",
  "publisher": "ZakyEvenso12",
  "repository": {
    "type": "git",
    "url": "https://github.com/faizinuha/Meiko"
  },
  "runtime": {
    "type": "node",
    "version": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc && webpack && npm run build-web",
    "build-web": "webpack --config webpack.web.config.js",
    "compile": "tsc -p ./",
    "format": "prettier --write .",
    "test": "node ./out/test/runTest.js",
    "watch": "tsc -watch"
  },
  "sponsor": {
    "url": "https://sociabuzz.com/zuax/tribe",
    "label": "Buy me a coffee"
  },
  "tags": [
    "Meiko",
    "codersHtml",
    "laravel",
    "AI",
    "snippets"
  ],
  "browser": "./dist/web/extension.js",
  "extensionDependencies": [
    "be5invis.vscode-custom-css"
  ],
  "version": "4.1.4"
}
