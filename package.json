{
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:php",
    "onLanguage:html",
    "onLanguage:java",
    "workspaceContains:**/*",
    "onCommand:extension.openLaravelAIGenerator",
    "onStartupFinished"
  ],
  "author": "Zaky",
  "badges": [
    {
      "description": "Meiko",
      "href": "https://github.com/faizinuha/Meiko",
      "url": "https://img.shields.io/badge/Meiko-Pro-blue"
    }
  ],
  "bin": {
    "coders-crud": "cli/crud-installer.js",
    "coders-delete": "cli/delete-solid.js",
    "coders-solid": "cli/coders-solid.js"
  },
  
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    },
    "virtualWorkspaces": true
  },
  "categories": [
    "Programming Languages",
    "AI",
    "Snippets",
    "Other"
  ],
  "contributes": {
    "documentation": {
      "releaseNotes": "./CHANGELOG.md"
    },
    "snippets": [
      {
        "language": "html",
        "path": "./src/html-snippets.json"
      },
      {
        "language": "php",
        "path": "./src/php-snippets.json"
      },
      {
        "language": "php",
        "path": "./src/laravel-snippets.json"
      },
      {
        "language": "java",
        "path": "./src/java-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./src/javascript-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./src/vue-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./src/ReactNative-snippets.json"
      },
      {
        "language": "python",
        "path": "./src/python-snippets.json"
      }
    ]
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "ora": "^8.1.1"
  },
  "description": "ðŸš€ Advanced AI-powered VS Code ",
  "devDependencies": {
    "@tensorflow/tfjs-node": "^4.4.0",
    "@types/node": "^22.8.4",
    "@types/vscode": "^1.95.0",
    "electron-builder": "^25.1.8",
    "openai": "^4.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.0.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "displayName": "Meiko",
  "engines": {
    "node": ">=14.0.0",
    "vscode": ">=1.95.0"
  },
  "icon": "assets/icons1.jpeg",
  "id": "ZakyEvenso12",
  "keywords": [
    "CodersLegacy",
    "php",
    "laravel",
    "AI",
    "javascript",
    "vue",
    "react",
    "snippets",
    "productivity",
    "web-development",
    "code-generator",
    "cli-tools",
    "typescript",
    "html"
  ],
  "license": "MIT",
  "main": "./out/extension.js",
  "name": "kawaiicode",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "pricing": "Free",
  "publisher": "ZakyEvenso12",
  "repository": {
    "type": "git",
    "url": "https://github.com/faizinuha/Meiko"
  },
  "runtime": {
    "type": "node",
    "version": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc && webpack && npm run build-web",
    "build-web": "webpack --config webpack.web.config.js",
    "compile": "tsc -p ./",
    "format": "prettier --write .",
    "test": "node ./out/test/runTest.js",
    "watch": "tsc -watch"
  },
  "sponsor": {
    "url": "https://ko-fi.com/mahiro885"
  },
  "tags": [
    "Meiko",
    "codersHtml",
    "laravel",
    "AI",
    "snippets"
  ],
  "browser": "./dist/web/extension.js",
  
  "extensionDependencies": [
    "be5invis.vscode-custom-css"
  ],
  "version": "3.4.4"
}
