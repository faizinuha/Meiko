  "React Native TextInput Component": {
    "prefix": "rnTextInput",
    "body": [
      "import React, { useState } from 'react';",
      "import { View, TextInput, Text } from 'react-native';",
      "",
      "const ${1:TextInputComponent} = () => {",
      "  const [text, setText] = useState('');",
      "",
      "  return (",
      "    <View style={{ padding: 10 }}>",
      "      <TextInput",
      "        placeholder=\"${2:Type here}\"",
      "        value={text}",
      "        onChangeText={setText}",
      "        style={{ borderWidth: 1, borderColor: 'gray', padding: 5 }}",
      "      />",
      "      <Text>${3:You typed:} {text}</Text>",
      "    </View>",
      "  );",
      "};",
      "",
      "export default ${1:TextInputComponent};"
    ],
    "description": "React Native component with a TextInput"
  },
